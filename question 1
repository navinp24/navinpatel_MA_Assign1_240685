
class Solution {
public:
    int majorityElement(vector<int> &num){
       int n1 ;
       int n = num.size();
       int count = 0;

       for(int i = 0; i< n; i++){
        if (count == 0){
            n1 = num[i];

        }
        if(num[i] == n1){
            count ++;

        }
        else{
            count--;
        }
       }
       return n1;
    }
};
